<%- include('../front/common/header') %>
<main class="main-wrapper">
          <!-- Start Checkout Area  -->
          <div class="axil-checkout-area axil-section-gap">
              <div class="container">
                  <form action="#">
                      <div class="row">
                          <div class="col-lg-12">
                              <div class="axil-order-summery order-checkout-summery">
                                  <h5 class="title mb--20">Select your Address</h5>
                                  <a href="/front/add_address" class="btn btn-info my-3 py-4"style="font-size:20px;">Add Address</a>
                                  <div class="summery-table-wrap">
                                      <table class="table summery-table">
                                          <thead>
                                              <tr>
                                                <th>Name</th>
                                                <th>Phone</th>
                                                <th>Pincode</th>
                                                <th>City</th>
                                               
                                                <th>Address</th>
                                                <th>Action</th>
                                              </tr>
                                          </thead>
                                          <tbody>
                                          <%   add_data.forEach((data)=>{  %> 
                                            <tr>
                                            <td><%=data.name%></td>
                                            <td><%=data.phone%></td>
                                            <td><%=data.pincode%></td>
                                            <td><%=data.city%></td>
                                            <td><%=data.address%></td>
                                            <td><a class="btn btn-info p-3"style="font-size:20px;"href="/front/checkout/<%=data._id%>">Select</a></td>
                                            </tr>
                                            <%})%>
                                          </tbody>
                                      </table>
                                  </div>
                                  </div>
                              </div>
                          </div>
                      </div>
              </div>
          </div>
          <!-- End Checkout Area  -->
  
      </main>
      <%- include('../front/common/footer') %> 